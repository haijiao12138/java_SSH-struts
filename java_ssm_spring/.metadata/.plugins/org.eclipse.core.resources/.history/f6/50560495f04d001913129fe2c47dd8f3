package com.sikiedu.test;

import org.junit.Test;

import com.sikiedu.bean.User;

public class HelloSpring {// 在之前项目开发中new对象是我们自己做的
	@Test
	public void Test1() {
		User u = new User();

		System.out.println(u);

	}
	
	//Spring 是一个容器 ,他帮我们管理对象
	@Test
	public void Test1() {
		User u = new User();

		System.out.println(u);

	}
	

}
